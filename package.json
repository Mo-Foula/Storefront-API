{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon -L src/server.ts",
    "_start": "node src/server.ts",
    "start_b": "node build/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "tsc": "tsc",
    "build": "npx tsc",
    "start_build": "node build/api/index.js",
    "format": "npx prettier --write src/**/*.ts",
    "lint": "npx eslint src/**/*.ts",
    "lint_f": "npx eslint --fix src/**/*.ts",
    "db:up": "db-migrate up",
    "db:down": "db-migrate reset",
    "test": "set ENV=test&& db-migrate db:create storefront_test && db-migrate --env test up && jasmine-ts || db-migrate db:drop storefront_test",
    "test_no_drop": "set ENV=test&& db-migrate db:create storefront_test && db-migrate --env test up && jasmine-ts",
    "drop_test_db": "db-migrate db:drop storefront_test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "2.7.1",
    "prettier-init": "^1.3.0",
    "ts-node": "^10.8.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.8.4"
  }
}
